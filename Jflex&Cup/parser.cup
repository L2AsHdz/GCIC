//**************************************** Sentencia if
ifStatement
    ::= simpleIf instructionBlock END ifTypes
    |   simpleIf oneStatement SEMI ifTypes
;

simpleIf
    ::= IF OPEN_ROUND_BRACKET condition CLOSE_ROUND_BRACKET THEN
;

ifTypes
    ::= ELSE instructionBlock END
    |   ELSE oneStatement SEMI
    |   ELSE ifStatement
    |
;

condition
    ::= condition EQUAL_TO condition
    |   condition NOT_EQTUAL_TO condition
    |   condition GREATER_THAN condition
    |   condition LESS_THAN condition
    |   condition GREATER_THAN_OR_EQUAL_TO condition
    |   condition LESS_THAN_OR_EQUAL_TO condition
    |   condition AND condition
    |   condition OR condition
    |   OPEN_ROUND_BRACKET condition CLOSE_ROUND_BRACKET
    |   OPEN_ROUND_BRACKET condition CLOSE_ROUND_BRACKET EQUAL_TO condition
    |   OPEN_ROUND_BRACKET condition CLOSE_ROUND_BRACKET NOT_EQTUAL_TO condition
    |   OPEN_ROUND_BRACKET condition CLOSE_ROUND_BRACKET GREATER_THAN condition
    |   OPEN_ROUND_BRACKET condition CLOSE_ROUND_BRACKET LESS_THAN condition
    |   OPEN_ROUND_BRACKET condition CLOSE_ROUND_BRACKET GREATER_THAN_OR_EQUAL_TO condition
    |   OPEN_ROUND_BRACKET condition CLOSE_ROUND_BRACKET LESS_THAN_OR_EQUAL_TO condition
